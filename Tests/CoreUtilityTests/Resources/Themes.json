{
	"type": "Themes",
    "identifier": "default_Theme",
    "osVersion": "*",
	"color": {
		"default": "#FFFFFF",
		"AA": "#16c5ce",
		"clear": "clear",
		"black": "#000000",
		"red": "#b70b39",
        "appRed": "#DD6060",
        "navBarRed": "#DF6E6E",
		"blue": "#1059ce",
		"green": "#10ce1a",
		"yellow": "#cecb10",
		"white": "#FFFFFF"
	},
	"font": {
		"default": {
			"name": "system",
			"size": "13.0"
		},
		"system14": {
			"_super": "default",
			"size": "14.0"
		},
        "system14W": {
            "_super": "default",
            "size": "14.0",
            "weight": 1
        },
        "systemB14": {
            "name": "boldSystem",
            "size": "14.0"
        },
        "systemI14": {
            "name": "italicSystem",
            "size": "14.0"
        }
	},
    "layer": {
        "roundRect": {
            "cornerRadius": 5,
            "borderWidth": 2,
            "borderColor": "red"
        },
        "elevated": {
            "masksToBounds": false,
            "shadowPath": true,
            "shadowColor": "blue",
            "shadowRadius": 5.0,
            "shadowOpacity": 0.6
        },
        "shadowOffset": {
            "shadowOffset": "{5}"
        }
    },
    "link": {
        "url": {
            "underlineColor": "blue",
            "foregroundColor": "blue"
        },
        "hash": {
            "underlineColor": "blue",
            "foregroundColor": "blue"
        }
    },
	"appearance": {
		"UINavigationBar:UINavigationController": {
			"barTintColor": "white",
			"tintColor": "white",
			"isTranslucent": false,
			"backgroundImage": {
				"default": "@TestImage"
			},
			"shadowImage": "@empty",
            "titleText": {
                "foregroundColor": "white"
            }
		},
		"UINavigationBar": {
			"barTintColor": "black",
			"isTranslucent": true
		}
	},
	"components": {
		"FTView": {
			"default": {
				"backgroundColor": "white"
			},
            "viewR": {
                "backgroundColor": "red"
            }
        },
        "UIView": {
            "default": {
                "backgroundColor": "white"
            },
            "elevated": {
                "_super": "default",
                "layer": "elevated"
            },
            "shadowOffset": {
                "_super": "default",
                "layer": "shadowOffset"
            }
		},
		"UIButton": {
			"default": {
				"textfont": "system14",
				"textcolor": "black",
				"backgroundColor": "clear",
				"layer": "roundRect"
			},
			"button14R": {
				"_super": "default",
				"textcolor": "red"
			},
			"button14R:highlighted": {
				"_super": "default",
				"textcolor": "green"
			},
			"button14R:disabled": {
				"_super": "default",
				"textcolor": "yellow"
			}
		},
		"UILabel": {
			"default": {
				"textfont": "system14",
				"textcolor": "black",
				"backgroundColor": "clear",
				"isLinkUnderlineEnabled": true,
				"isLinkDetectionEnabled": true
			},
			"system14R": {
				"_super": "default",
				"textcolor": "red"
			},
			"system14B": {
				"_super": "default",
				"textcolor": "blue"
			},
			"system14G": {
				"_super": "default",
				"textcolor": "green"
			},
			"system14Y": {
				"_super": "default",
				"textcolor": "yellow"
			},
			"system14AA": {
				"_super": "default",
				"textcolor": "AA"
			},
            "systemW14G": {
                "textfont": "system14W",
                "textcolor": "green"
            },
            "systemB14R": {
                "textfont": "systemB14",
                "textcolor": "red"
            },
            "systemI14R": {
                "textfont": "systemI14",
                "textcolor": "red"
            }
		},
        "UISegmentedControl": {
            "default": {
               "tintColor": "white",
               "iOS12Style": true,
               "ios_13": {
                   "tintColor": "navBarRed",
                   "textcolor": "white"
               }
            }
        },
        "UICollectionView": {
            "default": {
            },
            "collectionRed": {
                "backgroundViewTheme": "viewR"
            }
        },
        "SegmentCollectionView": {
            "default": {
                "backgroundColor": "clear"
            }
        },
        "UISearchBar": {
            "default": {
                "textcolor": "white",
                "tintColor": "white",
                "barTintColor": "navBarRed"
            }
        }
	}
}
